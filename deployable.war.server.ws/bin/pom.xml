        <version>5.5.0-RELEASE</version>
    </parent>
  <groupId>com.fideuram</groupId>
  <artifactId>deployable.war.server.ws</artifactId>
  <packaging>war</packaging>
 
  <name>deployable.war.server.ws</name>

	<properties>
		<!-- questo modulo e' deployato su TOMCAT 6 -->
		<java.source>1.6</java.source>
		<java.target>1.6</java.target>
	</properties>
    
  <dependencies>
  <dependency>
            <groupId>com.fideuram.crm.stampe</groupId>
            <artifactId>crm.stampe.engine</artifactId>
            <version>${project.build.rel}</version>
        </dependency>
        <dependency>
            <groupId>com.fideuram</groupId>
            <artifactId>modulo.bl.prospetto.offerta</artifactId>
            <version>${project.build.rel}</version>
        </dependency>
        <dependency>
            <groupId>com.fideuram</groupId>
            <artifactId>modulo.bl.collettive</artifactId>
            <version>${project.build.rel}</version>
        </dependency>
        <dependency>
            <groupId>com.fideuram</groupId>
            <artifactId>modulo.bl.redditi</artifactId>
            <version>${project.build.rel}</version>
        </dependency>
       <!--
       <dependency>
            <groupId>com.fideuram</groupId>
            <artifactId>modulo.bl.cud</artifactId>
            <version>${project.build.rel}</version>
        </dependency>
        -->
        <dependency>
            <groupId>com.fideuram</groupId>
            <artifactId>modulo.bl.asset</artifactId>
            <version>${project.build.rel}</version>
        </dependency>
        <dependency>
            <groupId>com.fideuram</groupId>
            <artifactId>modulo.bl.trasformazioni</artifactId>
            <version>${project.build.rel}</version>
        </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>modulo.sys.stampe</artifactId>
          <version>${project.build.rel}</version>
      </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>modulo.bl.stampe.generics</artifactId>
          <version>${project.build.rel}</version>
      </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>modulo.bl.prospetto.offerta</artifactId>
          <version>${project.build.rel}</version>
          <exclusions>
      			<exclusion>
        			<groupId>com.fideuram</groupId>
        			<artifactId>client.ws.stampeFv</artifactId>
      			</exclusion>
      			<exclusion>
        			<groupId>com.fideuram</groupId>
        			<artifactId>client.ws.puc</artifactId>
      			</exclusion>
    		</exclusions>
      </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>crm.stampe.fv.scampoli.xslt</artifactId>
          <version>${project.build.rel}</version>
      </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>modulo.bl.display.polizza</artifactId>
          <version>${project.build.rel}</version>
      </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>modulo.bl.credenziali</artifactId>
          <version>${project.build.rel}</version>
      </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>modulo.bl.preventivi</artifactId>
          <version>${project.build.rel}</version>
      </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>modulo.bl.asset</artifactId>
          <version>${project.build.rel}</version>
      </dependency>
      <dependency>
          <groupId>com.fideuram</groupId>
          <artifactId>modulo.bl.trasformazioni</artifactId>
